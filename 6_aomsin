#Data Type
name =  "Nuttanicha boonuan"
age = 14
sex = "women"
student = True
list_name = ["eva","mudmee","namnueng","grace","mew"]


print(type(name),name)
print(type (age),age)
print(type(sex),sex)
print(type(student),student)
print(list_name[0])
print(list_name[3])

#operator
